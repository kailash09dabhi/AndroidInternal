<manifest
    package="com.kingbull.internal"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kingbull.android.internal.ui.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.kingbull.android.internal.ui.view.HomeActivity"/>
        <activity android:name="com.kingbull.android.internal.ui.view.FieldsActivity"/>
        <activity android:name="com.kingbull.android.internal.ui.view.MethodsActivity"/>
        <activity android:name="com.kingbull.android.internal.ui.view.InheritanceActivity"/>


    </application>

</manifest>
